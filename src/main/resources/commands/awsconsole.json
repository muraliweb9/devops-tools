      "shortcut": null,
      "code": "netstat -tulpn",
      "example": "netstat -tulpn"
    },
    {
      "command": "sudo ss -lntp",
      "desc": "Find ports occupied",
      "shortcut": null,
      "code": "sudo ss -lntp",
      "example": "sudo ss -lntp"
    },
    {
      "command": "curl local port",
      "desc": "Run curl on local port",
      "shortcut": null,
      "code": "curl http://localhost:8080",
