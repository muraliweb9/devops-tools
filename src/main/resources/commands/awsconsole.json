      "shortcut": null,
      "code": "sudo yum install java-17-amazon-corretto.x86_64",
      "example": "sudo yum install java-17-amazon-corretto.x86_64"
    },
    {
      "command": "sudo yum remove java-17",
      "desc": "Remove Java 17",
      "shortcut": null,
      "code": "sudo yum remove java-17-amazon-corretto.x86_64",
      "example": "sudo yum remove java-17-amazon-corretto.x86_64"
    },
    {
      "command": "alternatives --config java",
      "desc": "List (so can choose) available Java versions",
      "shortcut": null,
      "code": "alternatives --config java",
      "example": "alternatives --config java"
    },
    {
