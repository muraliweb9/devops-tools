{
  "tool": "AWS Console",
  "shorthand": "awsn",
  "commands": [
    {
      "command": "whoami",
      "desc": "Find current user",
      "shortcut": null,
      "code": "whoami",
      "example": "whoami"
    },
    {
      "command": "sudo -i",
      "desc": "Become root user",
      "shortcut": null,
      "code": "sudo -i",
      "example": "sudo -i"
    },
    {
      "command": "yum list java*",
      "desc": "List all java packages available to install",
      "shortcut": null,
      "code": "yum list java*",
      "example": "yum list java*"
    },
    {
      "command": "yum list java* ^| grep 17",
      "desc": "List all java 17 packages available to install",
      "shortcut": null,
      "code": "yum list java* ^| grep 17",
      "example": "yum list java* ^| grep 17"
    },
    {
