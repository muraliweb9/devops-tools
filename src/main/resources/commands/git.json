{
  "tool": "GIT",
  "shorthand": "g",
  "commands": [
    {
      "command": "git checkout",
      "desc": "Checkout a branch",
      "shortcut": "gc",
      "code": "git checkout %1",
      "example": "gc main"
    },
    {
      "command": "git status",
      "desc": "Local status",
      "shortcut": "gs",
      "code": "git status",
      "example": "gs"
    },
    {
      "command": "git push origin",
      "desc": "Push to origin",
      "shortcut": "gph",
      "code": "git push origin %1:%1",
      "example": "gph feature/FT1"
    },
    {
      "command": "git pull",
      "desc": "Pull origin",
      "shortcut": "gp",
      "code": "git pull",
      "example": "gp"
    },
    {
      "command": "git branch",
      "desc": "List branches",
      "shortcut": "gb",
      "code": "git branch",
      "example": "gb"
    },
    {
      "command": "git log",
      "desc": "Log history",
      "shortcut": "glo",
      "code": "git log --oneline",
      "example": "glo"
    },
    {
      "command": "git reset",
      "desc": "Git reset local changes",
      "shortcut": "gr",
      "code": "git reset --hard HEAD",
      "example": "gr"
    },
    {
      "command": "git repo",
      "desc": "Show the remote repo",
      "shortcut": "grepo",
      "code": "git config --get remote.origin.url",
      "example": "grepo"
    },
    {
      "command": "git merge",
      "desc": "Merge a branch to current branch",
      "shortcut": "gm",
      "code": "git merge %1",
      "example": "gm main"
    },
    {
      "command": "git config",
      "desc": "List the git config",
      "shortcut": "gconfig",
      "code": "git config --list",
      "example": "gconfig"
    },
    {
      "command": "git branch (new)",
      "desc": "Create a new branch from existing",
      "shortcut": "gbm",
      "code": "git branch %1 %2",
      "example": "gbm new main"
    }
  ]
}